*
  padding: 0;
  margin: 0;
body, html, #app
  width: 100%;
  height: 100%;
input,button,select,textarea
  outline none
input, textarea, select
  border 1px solid #dfdfdf
  border-radius 2px
  padding 0 3px
textarea
  resize none
  padding 2px 3px
select
  height 29px
input
  line-height 30px
.clearfix
  &:after
    content ''
    display block
    clear both
.dis-flex
  display flex
  .w-80
    width 80px
    text-align right
    padding-right 5px
  .flex
    flex 1
li
  list-style none

.todo-content
  height 100%
  background-color: #f5f5f5;
.func-ment
  position fixed
  top 0
  left 0
  background #fff
  box-shadow 1px 1px 1px #ccc
  height 100%
  width 90px
  .li
    padding 0 10px
    line-height 25px
    cursor pointer